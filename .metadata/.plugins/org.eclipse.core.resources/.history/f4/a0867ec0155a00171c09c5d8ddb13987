<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
<!-- get 방식 "/join2/idCheck" 요청처리 -->
app.get("/friends/insert.jps/phoneCheck", function(req, res){
	<!-- 전송된 파라미터 추출 -->
	var id = req.query.inputId; <!-- 입력한 아이디 -->
	if(id==/^\d{3}-\d{3,4}-\d{4}$/){<!--폰 번호 정규식 -->
		res.json({canUse:true});
	}else{
		res.json({canUse:false});
	}
});
    