<!-- get 방식 "/join2/idCheck" 요청처리 -->
app.get("/friends/insert.jps/phoneCheck", function(req, res){
	<!-- 전송된 파라미터 추출 -->
	var phone = req.query.inputPhone; <!-- 입력한 아이디 -->
	if(phone==/^\d{3}-\d{3,4}-\d{4}$/){<!--폰 번호 정규식 -->
		true;
	}
