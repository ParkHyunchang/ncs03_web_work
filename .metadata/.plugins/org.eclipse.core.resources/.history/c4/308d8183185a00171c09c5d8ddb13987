<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<script>
function(req, res){
	<!-- 전송된 파라미터 추출 -->
	var phone = req.query.inputPhone; <!-- 입력한 아이디 -->
	if(phone==/^\d{3}-\d{3,4}-\d{4}$/){<!--폰 번호 정규식 -->
		res.json({canUse:false});
	}else{
		res.json({canUse:true});
}
</script>